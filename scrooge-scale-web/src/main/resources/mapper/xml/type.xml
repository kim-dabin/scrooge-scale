<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper 
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.kdb.scroogescaleweb.mapper.TypeMapper">

	<select id="selectOne" parameterType="int" resultType="Type">
		select id, name, img, detail
		from type
		where id=#{id}
	</select>
</mapper>